{
    "[vue]": {
        "editor.defaultFormatter": "Vue.volar"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript",
        "typescript",
        "vue",
        "html"
    ],
    "cSpell.words": [
        "ARGB",
        "gapi",
        "Leds",
        "NETLED"
    ]
}